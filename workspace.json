{
  "version": 2,
  "projects": {
    "esm-cookie-service": {
      "root": "libs/esm-cookie-service",
      "sourceRoot": "libs/esm-cookie-service/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/esm-cookie-service",
            "tsConfig": "libs/esm-cookie-service/tsconfig.lib.json",
            "project": "libs/esm-cookie-service/package.json",
            "entryFile": "libs/esm-cookie-service/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/esm-cookie-service/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/esm-cookie-service/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/esm-cookie-service"],
          "options": {
            "jestConfig": "libs/esm-cookie-service/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-cookie-service": {
      "root": "libs/react-cookie-service",
      "sourceRoot": "libs/react-cookie-service/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/react-cookie-service",
            "tsConfig": "libs/react-cookie-service/tsconfig.lib.json",
            "project": "libs/react-cookie-service/package.json",
            "entryFile": "libs/react-cookie-service/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/react-cookie-service/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/react-cookie-service/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/react-cookie-service"],
          "options": {
            "jestConfig": "libs/react-cookie-service/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-cookie-service-example": {
      "root": "apps/react-cookie-service-example",
      "sourceRoot": "apps/react-cookie-service-example/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/react-cookie-service-example",
            "index": "apps/react-cookie-service-example/src/index.html",
            "main": "apps/react-cookie-service-example/src/main.tsx",
            "polyfills": "apps/react-cookie-service-example/src/polyfills.ts",
            "tsConfig": "apps/react-cookie-service-example/tsconfig.app.json",
            "assets": [
              "apps/react-cookie-service-example/src/favicon.ico",
              "apps/react-cookie-service-example/src/assets"
            ],
            "styles": ["apps/react-cookie-service-example/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react-cookie-service-example/src/environments/environment.ts",
                  "with": "apps/react-cookie-service-example/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-cookie-service-example:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "react-cookie-service-example:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/react-cookie-service-example/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/react-cookie-service-example"],
          "options": {
            "jestConfig": "apps/react-cookie-service-example/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-cookie-service-example-e2e": {
      "root": "apps/react-cookie-service-example-e2e",
      "sourceRoot": "apps/react-cookie-service-example-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-cookie-service-example-e2e/cypress.json",
            "tsConfig": "apps/react-cookie-service-example-e2e/tsconfig.e2e.json",
            "devServerTarget": "react-cookie-service-example:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react-cookie-service-example:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/react-cookie-service-example-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "react-cookie-service-example"
}
